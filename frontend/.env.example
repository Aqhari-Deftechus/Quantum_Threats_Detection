# Keep empty to use Vite dev proxy for API calls.
# Example: VITE_API_BASE_URL=http://127.0.0.1:8010
VITE_API_BASE_URL=

# Keep empty to use same host as frontend (through Vite WS proxy).
# Example: VITE_WS_BASE_URL=ws://127.0.0.1:8010
VITE_WS_BASE_URL=

# Disable WHEP unless MediaMTX (or equivalent) is running.
VITE_ENABLE_WHEP=false

# Only used when VITE_ENABLE_WHEP=true.
VITE_MEDIAMTX_WHEP_BASE_URL=http://127.0.0.1:8889
VITE_MEDIAMTX_WHEP_PATH_TEMPLATE=camera-{camera_id}
